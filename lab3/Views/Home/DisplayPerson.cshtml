@model Lab3.Models.Person
@{
    Layout = "_Layout";
    
}


<h3>Display Person Data</h3>
<table>
    <tr>
        <td>
            First Name:
        </td>
        <td>
            @Model.firstName
        </td>
    </tr>

    <tr>
        <td>
            Last Name:
        </td>
        <td>
            @Model.lastName
        </td>
    </tr>

    <tr>
        <td>
            Age:
        </td>
        <td>
            @Model.age
        </td>
    </tr>

    <tr>
        <td>
            Email Address:
        </td>
        <td>
            @Model.emailAddress
        </td>
    </tr>

    <tr>
        <td>
            Date of Birth:
        </td>
        <td>
            @Model.dateOfBirth
        </td>
    </tr>

    <tr>
        <td>
            Password:
        </td>
        <td>
            @Model.password
        </td>
    </tr>

    <tr>
        <td>
            Description:
        </td>
        <td>
            @Model.description
        </td>
    </tr>
</table>
